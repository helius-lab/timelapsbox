{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Capture Series",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/capture_series.js",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "env": {
        "TOTAL_TIME_MINUTES": "1",
        "TOTAL_PHOTOS": "10"
      }
    }
  ]
}
